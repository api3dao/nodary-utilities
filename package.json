{
  "name": "@nodary/utilities",
  "description": "Utility functions for Nodary services",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/api3dao/nodary-utilities.git"
  },
  "version": "7.0.0",
  "private": false,
  "main": "src/index.js",
  "files": [
    "data",
    "src"
  ],
  "scripts": {
    "changeset": "changeset",
    "release": "yarn changeset publish",
    "lint": "yarn run prettier:check && yarn run lint:eslint",
    "lint:eslint": "eslint . --ext .js,.ts",
    "prettier:check": "prettier --check \"./**/*.{js,ts,md,json,sol}\"",
    "prettier": "prettier --write \"./**/*.{js,ts,md,json,sol}\"",
    "validate:dapi-management-coverage": "node scripts/validate-dapi-management-coverage.js"
  },
  "devDependencies": {
    "eslint": "^10.0.1",
    "prettier": "^3.8.1"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=24.13.1"
  },
  "dependencies": {
    "@api3/airnode-abi": "^0.15.0",
    "@api3/dapi-management": "^4.7.0",
    "@changesets/cli": "^2.29.8",
    "ethers": "^6.16.0"
  }
}
